{
	"name": "personal-site",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "rsbuild build",
		"dev": "rsbuild dev --open",
		"lint:ts": "tsc -p tsconfig.json --noEmit",
		"check": "biome check --write",
		"fix": "biome lint --fix",
		"fix:unsafe": "pnpm run fix --unsafe",
		"format": "biome format --write",
		"preview": "rsbuild preview",
		"prepare": "lefthook install",
		"upgrade-packages": "pnpm up --latest --interactive"
	},
	"dependencies": {
		"@tanstack/react-router": "^1.139.12",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"react-icons": "^5.5.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.8",
		"@rsbuild/core": "^1.6.7",
		"@rsbuild/plugin-react": "^1.4.2",
		"@tailwindcss/postcss": "^4.1.17",
		"@tanstack/router-plugin": "^1.139.12",
		"@types/react": "^19.2.6",
		"@types/react-dom": "^19.2.3",
		"lefthook": "^2.0.4",
		"react-ga4": "^2.1.0",
		"tailwindcss": "^4.1.17",
		"typescript": "^5.9.3"
	}
}
